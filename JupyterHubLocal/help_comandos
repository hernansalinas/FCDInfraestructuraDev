#El archivo docker-compose contiene informacion de lo que se va a construir 
#con el archivo dockerfilejupyter hub

DOCKER_BUILDKIT=0 docker-compose up -d --build #ejecunta el docker compose

docker ps # Lista los contenedores

docker exe --it <name_contenedores> bash
docker log <name_contendor >

useradd -m -p $(openssl passwd -1 password) username
useradd -m -p $(openssl passwd -1 1234567890) pedro

#Ver usuarios dentro de  jupyure hub
#sqlite3 jupyterhub.sqlite

copiar archivos al directorio principal
docker cp jupyterhub_simple:/home/ holamundo
